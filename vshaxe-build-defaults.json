{
    "haxelibs": [
        {
            "name": "hxnodejs",
            "installArgs": ["git", "hxnodejs", "https://github.com/HaxeFoundation/hxnodejs"]
        },
        {
            "name": "jstack",
            "installArgs": ["install", "jstack"]
        }
    ],
    "targets": [
        {
            "name": "empty"
        },
        {
            "name": "vshaxe-node",
            "debug": {
                "args": {
                    "defines": ["js_unflatten"],
                    "haxelibs": []
                }
            },
            "args": {
                "haxelibs": ["hxnodejs"],
                "deadCodeElimination": "full",
                "defines": ["analyzer-optimize", "js-enums-as-arrays"]
            }
        }
    ]
}