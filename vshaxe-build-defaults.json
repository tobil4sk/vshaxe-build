{
	"haxelibs": [
		{
			"name": "hxnodejs",
			"installArgs": ["git", "hxnodejs", "https://github.com/HaxeFoundation/hxnodejs"]
		},
		{
			"name": "jstack",
			"installArgs": ["git", "jstack", "https://github.com/RealyUniqueName/JStack"]
		}
	],
	"targets": [
		{
			"name": "empty"
		},
		{
			"name": "vshaxe-node",
			"debug": {
				"args": {
					"defines": ["js_unflatten"],
					"haxelibs": ["jstack"]
				}
			},
			"args": {
				"haxelibs": ["hxnodejs"],
				"deadCodeElimination": "full",
				"defines": ["analyzer-optimize", "js-enums-as-arrays", "js-es=6"]
			}
		}
	]
}