{
    "mainTarget": "run-script",
    "haxelibs": [
        {
            "name": "hxargs",
            "installArgs": ["install", "hxargs"]
        },
        {
            "name": "json2object",
            "installArgs": ["git", "json2object", "https://github.com/elnabo/json2object"]
        }
    ],
    "targets": [
        {
            "name": "run-script",
            "args": {
                "classPaths": ["src", "api"],
                "haxelibs": ["hxargs", "json2object"],
                "main": "vshaxeBuild.Main",
                "output": {
                    "target": "neko",
                    "path": "run.n"
                },
                "debug": true
            },
            "isBuildCommand": true
        }
    ]
}